{
  "expo": {
    "name": "sunshine-action-app",
    "slug": "sunshine-action-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAiYlZd92wGPRDVlcMgI0WqxTUXAjUeIe8"
      }
    },
    "android": {
      "config":{
        "googleMaps": { 
          "apiKey": "AIzaSyAiYlZd92wGPRDVlcMgI0WqxTUXAjUeIe8"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "sourceExts": ["cjs"]
    }
  }
}
